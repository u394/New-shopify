<h1>main-page-custom-product</h1>
{% assign image_count = page.metafields.Image.Image.size | minus: 1 %}

{% for i in (0..image_count) %}
  <div class="new-div">
    <img src="{{ page.metafields.Image.Image[i] }}" alt="Image {{ i }}" />
    <p>{{ page.metafields.custom-product-text.custom-product-text[i] }}</p>
  </div>
{% endfor %}



{% schema %}
  {
    "name": "Section name",
    "settings": []
  }
{% endschema %}